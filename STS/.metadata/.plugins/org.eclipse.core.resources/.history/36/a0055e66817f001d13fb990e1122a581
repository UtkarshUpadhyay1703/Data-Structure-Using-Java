package com.bst;

public class Main {
	public static void main() {
		BST bst = new BST();
		bst.insert(21);
		System.out.println("------");
		bst.insert(15);
		System.out.println("------");
		bst.insert(13);
		System.out.println("------");
		bst.insert(9);
		System.out.println("------");
		bst.insert(28);
		
		System.out.println("----------");
		bst.preOrder();
		
		System.out.println("------------");
		bst.inOrder();
		
		System.out.println("--------------");
		bst.postOrder();
	}
}
